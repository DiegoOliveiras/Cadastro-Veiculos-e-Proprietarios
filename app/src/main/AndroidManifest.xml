<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diego.cadastroveiculoseproprietarios">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.example.diego.cadastroveiculoseproprietarios" />

        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.NovoVeiculo" />
        <activity android:name=".Activity.NovoProprietario" />
        <activity android:name=".Activity.VerProprietarios" />
        <activity android:name=".Activity.Proprietario" />
        <activity android:name=".Activity.EditarProprietario" />
        <activity android:name=".Activity.EditarVeiculo"></activity>
    </application>

</manifest>